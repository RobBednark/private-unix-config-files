# Tixie servers:
Host in.tixie.org
  IdentityFile ~/git/tixie-web/keys/tixie.pem
  User ubuntu
Host *.tixie.org
  IdentityFile ~/git/tixie-web/keys/tixie.pem
  User ubuntu

# Host github.com
#     IdentityFile ~/.ssh/id_rsa

# Trapit servers:

# trapit.com hosts
Host a-build-0 c-astage-1 mon ci-build-0
    HostName %h.trapit.com
    User rbednark
    ForwardAgent yes

Host mon
    HostName mon.trapit.com
    User root
    ForwardAgent yes

# Higgs hosts:
Host db-1 db-2 db-3 db-4 ez-1 ez-2 ez-3 ez-4 ez-5 ez-6 ez-7 ez-8 ez-9 lab-1 logdb-1 mom-1 mom-2 mom-3 nb-1 short-1 www-4 xm-2 zb-1 zb-2 zb-3 
    # Note that %h expands to the hostname
    HostName %h.higgs.trapit.com
    User rbednark
    ForwardAgent yes

Host images
    # Note that %h expands to the hostname
    HostName %h.higgs.trap.it
    User rbednark
    ForwardAgent yes

